// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {}
//marcosx
model UserProjects {}

model Project {
    id          Int @id @default(autoincrement())
    title       String
    description String
    manager     String
    start_date  DateTime
    end_date    DateTime
    subject     String
    status      String
    tags        String?
    att         String?
    
}



model Members { }


model Login { }

